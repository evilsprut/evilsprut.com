{
  "name": "bukhalo-blog",
  "private": true,
  "description": "Bukhalo Blog",
  "version": "1.0.0",
  "author": "Aleksandr Bukhalo <aleksandr@bukhalo.com>",
  "bugs": {
    "url": "https://github.com/bukhalo/blog.bukhalo.com/issues"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.0.29",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.2.10",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "husky": "^3.0.2",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "numeralize-ru": "^1.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "styled-components": "^4.1.3",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
    "now": "^16.1.1",
    "prettier": "^1.18.2"
  },
  "homepage": "https://blog.bukhalo.com",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "deploy": "yarn build && cp now.json public/ && cd public && now alias $(now) overreacted.io",
    "now-build": "yarn build",
    "dry": "yarn build && cp now.json public/ && cd public && now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
