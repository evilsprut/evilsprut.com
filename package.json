{
  "name": "@bukhalo/bukhalo.com",
  "private": true,
  "description": "My personal site",
  "version": "1.1.0",
  "author": "Aleksandr Bukhalo <aleksandr@bukhalo.com>",
  "bugs": {
    "url": "https://github.com/bukhalo/bukhalo.com/issues"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.7",
    "gatsby": "2.19.24",
    "gatsby-plugin-feed": "2.3.27",
    "gatsby-plugin-google-analytics": "2.1.36",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-styled-components": "3.1.19",
    "gatsby-plugin-typescript": "2.2.0",
    "gatsby-plugin-typography": "2.3.22",
    "gatsby-remark-autolink-headers": "2.1.24",
    "gatsby-remark-copy-linked-files": "2.1.37",
    "gatsby-remark-images": "3.1.44",
    "gatsby-remark-prismjs": "3.3.31",
    "gatsby-remark-responsive-iframe": "2.2.32",
    "gatsby-remark-smartypants": "2.1.21",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-remark": "2.6.53",
    "gatsby-transformer-sharp": "2.3.16",
    "husky": "3.1.0",
    "lint-staged": "8.2.1",
    "lodash": "4.17.15",
    "numeralize-ru": "1.0.1",
    "prismjs": "1.19.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "styled-components": "4.4.1",
    "typography": "0.16.19",
    "typography-theme-wordpress-2016": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.8.6",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/react": "5.3.14",
    "@types/node": "12.12.29",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "babel-loader": "8.0.6",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.18.3",
    "gh-pages": "2.2.0",
    "now": "16.7.3",
    "prettier": "1.19.1"
  },
  "homepage": "https://bukhalo.com",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,.ts,.tsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "deploy": "npm run build && cp now.json public/ && cd public && now alias $(now) bukhalo.com",
    "now-build": "npm run build",
    "dry": "npm run build && cp now.json public/ && cd public && now",
    "storybook": "NODE_ENV=production start-storybook -s public",
    "build-storybook": "NODE_ENV=production  build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "npm run format",
      "git add"
    ]
  }
}
